<h1>Registrazione</h1>

<form action="registrazione_execute" method="post">
    <div class="form-group">
        <label for="nome">Nome</label>
        <input type="text" class="form-control" name="nome" value="{{nome}}" />
    </div>
    <div class="form-group">
        <label for="cognome">Cognome</label>
        <input type="text" class="form-control" name="cognome" value="{{cognome}}" />
    </div>
    <div class="form-group">
        <label for="email">Email</label>
        <input type="text" class="form-control" name="email" value="{{email}}" />
    </div>
    <div class="form-group">
        <label for="password">Password</label>
        <input type="text" class="form-control" name="password" value="" />
    </div>
    <div class="form-group">
        <label for="password2">Conferma password</label>
        <input type="text" class="form-control" name="password2" value="" />
    </div>
    <button type="submit" class="btn btn-success" name="aggiungi">Registrati</button>
</form>

